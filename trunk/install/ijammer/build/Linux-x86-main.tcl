set val $_args(-value)

if {$lang eq "None"} {
set ::info($var) $val
} else {
foreach lang $langs {
::msgcat::mcset $lang $var $val
}
if {[info exists cmds($var)]} {
uplevel #0 $cmds($var)
}
}
}

if {[info exists _args(-action)] && $_args(-action) ne ""} {
