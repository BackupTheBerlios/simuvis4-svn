proc ::InstallJammer::PauseCheck {} {
global conf
global info

if {$info(InstallStopped)} { return 0 }

while {[file exists $conf(pause)]} {
if {[file exists $conf(stop)]} {
set info(InstallStopped) 1
return 0
}
after 500
}

return 1
}

