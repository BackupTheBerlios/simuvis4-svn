}

::InstallJammer::ParseCommandLineArguments $::argv

::InstallJammer::LoadTwapi

if {$info(GuiMode)} {
SourceCachedFile gui.tcl
InitGui
}

::InstallJammer::ConfigureBidiFonts
}

